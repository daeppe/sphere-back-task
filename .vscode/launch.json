    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "dev",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "run",
                "start:dev"
            ],
            "runtimeExecutable": "yarn",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/src/server.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}